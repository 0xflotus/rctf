{
  "name": "rctf",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "_lint": "sh -c 'tsc --noEmit -p $0 ; eslint $0 \"$@\"'",
    "lint:server": "yarn _lint server",
    "lint:client": "yarn _lint client",
    "lint": "sh -c 'yarn lint:client \"$@\"; yarn lint:server \"$@\"' lint",
    "start": "node --enable-source-maps --unhandled-rejections=strict dist/server/index.js",
    "migrate": "yarn build:ts && cross-env RCTF_DATABASE_MIGRATE=only yarn start",
    "build:client": "webpack-cli --context client --config client/webpack.config.js --env production",
    "build:ts": "tsc -p server && yarn copy-static",
    "build": "yarn build:ts && yarn build:client",
    "watch:client": "webpack-dev-server --context client --config client/webpack.config.js --env development",
    "watch:server": "nodemon --enable-source-maps dist/server/index.js | pino-pretty -c -t",
    "watch:ts": "tsc -w -p server",
    "dev": "yarn build:ts && concurrently -k -t \"HH:mm:ss.SSS\" -p \"[{time}]\" -c \"cyan,green,red\" \"yarn:watch:*\"",
    "test": "yarn build && nyc --nycrc-path server/.nycrc.json --skip-full ava",
    "test:slim": "ava",
    "test:report": "nyc --nycrc-path server/.nycrc.json --skip-full --reporter=lcov ava",
    "copy-static": "cpy --cwd=server/src \"**/*\" \"!**/*.{j,t}s\" ../../dist/server --parents && cpy migrations dist/ --parents"
  },
  "nodemonConfig": {
    "watch": [
      "dist/server",
      "dist/config",
      "dist/build/index.html"
    ],
    "delay": "1000"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@google-cloud/storage": "5.1.2",
    "aws-sdk": "2.726.0",
    "content-disposition": "0.5.3",
    "data-uri-to-buffer": "3.0.1",
    "deepmerge": "4.2.2",
    "dotenv": "8.2.0",
    "email-validator": "2.0.4",
    "fastify": "3.1.1",
    "fastify-helmet": "4.0.2",
    "fastify-static": "3.2.0",
    "got": "11.5.1",
    "hyperid": "2.0.5",
    "mailgun-js": "0.22.0",
    "mustache": "4.0.1",
    "node-pg-migrate": "5.3.0",
    "nodemailer": "6.4.11",
    "pg": "8.3.0",
    "redis": "3.0.2",
    "uuid": "8.3.0",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@prefresh/webpack": "0.8.2",
    "@types/content-disposition": "0.5.3",
    "@types/mailgun-js": "0.22.10",
    "@types/mustache": "4.0.1",
    "@types/node": "14.0.27",
    "@types/nodemailer": "6.4.0",
    "@types/pg": "7.14.4",
    "@types/react": "16.9.44",
    "@types/theme-ui": "0.3.6",
    "@types/uuid": "8.0.1",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.8.0",
    "ava": "3.11.1",
    "babel-loader": "8.1.0",
    "concurrently": "5.2.0",
    "copy-webpack-plugin": "6.0.3",
    "cpy-cli": "3.1.1",
    "cross-env": "7.0.2",
    "eslint": "7.6.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-ava": "10.5.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "eslint-plugin-standard": "4.0.1",
    "faker": "4.1.0",
    "fork-ts-checker-webpack-plugin": "5.0.14",
    "get-port": "5.1.1",
    "glob": "7.1.6",
    "history": "5.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "pino-pretty": "4.1.0",
    "preact": "10.4.6",
    "react-refresh": "0.8.3",
    "react-router-dom": "6.0.0-beta.0",
    "supertest": "4.0.2",
    "terser-webpack-plugin": "4.0.0",
    "theme-ui": "0.3.1",
    "type-fest": "0.16.0",
    "typescript": "3.9.7",
    "webpack": "5.0.0-beta.24",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "description": "rCTF is redpwnCTF's CTF platform. It is developed and maintained by the [redpwn](https://redpwn.net) CTF team.",
